<?xml version="1.0" encoding="UTF-8"?>
<model name="rmpsy:psychModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <!-- Optional meta-data about the model -->
    <description>Model the Region Midts databaser</description>
    <author>Magenta ApS</author>
    <version>1.0</version>

    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <!-- Import Alfresco Site Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/site/1.0" prefix="st"/>
    </imports>

    <!-- Custom namespace for Region Midt -->
    <namespaces>
        <namespace uri="http://www.rm.dk/model/psycdatabase/1.0" prefix="rmpsy"/>
    </namespaces>

    <!-- Constraints -->

    <!-- Types -->

    <types>
        <type name="rmpsy:anvendtUndersoegelsesinst">
            <title>Anvendte undersøgelsesinstrumenter</title>
            <parent>cm:content</parent>
            <properties>
                <property name="rmpsy:id_anvendtUndersoegelsesinst">
                    <title>id</title>
                    <type>d:text</type>
                </property>
                <property name="rmpsy:name_anvendtUndersoegelsesinst">
                    <title>name</title>
                    <type>d:text</type>
                </property>
            </properties>
        </type>

    </types>

    <!-- Aspects -->
    <aspects>

        <aspect name="rmpsy:psycTypeCounter">
            <title>primary key for the psyctypes</title>
            <properties>

            <property name="rmpsy:psykologisk_undersoegelsestype_counter">
                <title>counter</title>
                <type>d:int</type>
            </property>
            </properties>
        </aspect>


        <aspect name="rmpsy:psyc">
            <title>psyc data</title>
            <properties>

                <property name="rmpsy:psykologisk_undersoegelsestype">
                    <title>Psykologisk undersøgelsestype</title>
                    <type>d:text</type>
                    <multiple>true</multiple>
                </property>

                <property name="rmpsy:psykiatriske_interviews_og_ratingscales">
                    <title>Psykiatriske interviews og ratingscales</title>
                    <type>d:text</type>
                    <multiple>true</multiple>
                </property>

                <property name="rmpsy:kognitive_og_neuropsykologiske_praestationstests">
                    <title>Kognitive_og_neuropsykologiske_praestationstests</title>
                    <type>d:text</type>
                    <multiple>true</multiple>
                </property>

                <property name="rmpsy:implicitte_projektive_tests">
                    <title>Implicitte_projektive_tests</title>
                    <type>d:text</type>
                    <multiple>true</multiple>
                </property>

                <property name="rmpsy:eksplicitte_spoergeskema_tests">
                    <title>Eksplicitte_spoergeskema_tests</title>
                    <type>d:text</type>
                    <multiple>true</multiple>
                </property>

                <property name="rmpsy:instrumenter_for_indikation_på_malingering">
                    <title>Instrumenter_for_indikation_på_malingering</title>
                    <type>d:text</type>
                    <multiple>true</multiple>
                </property>

                <property name="rmpsy:risikovurderingsinstrumenter">
                    <title>Risikovurderingsinstrumenter</title>
                    <type>d:text</type>
                    <multiple>true</multiple>
                </property>

                <property name="rmpsy:psykologisk_vurdering_af_forekomst_af_malingering">
                    <title>Psykologisk_vurdering_af_forekomst_af_malingering</title>
                    <type>d:text</type>
                    <multiple>true</multiple>
                </property>

                <property name="rmpsy:konklusion_tags">
                    <title>Konklusion_tags</title>
                    <type>d:text</type>
                    <multiple>true</multiple>
                </property>

                <property name="rmpsy:konklusion_freetext">
                    <title>Konklusion_freetext</title>
                    <type>d:text</type>
                    <multiple>false</multiple>
                </property>


            </properties>
        </aspect>


    </aspects>
</model>
